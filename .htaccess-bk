<IfModule mod_rewrite.c>
RewriteEngine on
RewriteCond %{HTTP_HOST} ^sounds.bunkertea.club$
RewriteCond %{REQUEST_URI} !^/Laracast/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /Laracast/$1
RewriteCond %{HTTP_HOST} ^sounds.bunkertea.club$
RewriteRule ^(/)?$ Laracast/index.php [L] 
</IfModule>
